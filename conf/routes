# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                    controllers.EyeSocketController.socket

GET        /info                controllers.EyeSocketController.info

GET        /config              controllers.EyeSocketController.config

GET        /sse                 controllers.EyeSocketController.sse

GET        /socket              controllers.EyeSocketController.socket

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file        controllers.Assets.at(path="/public", file)
